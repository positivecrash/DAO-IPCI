						span.btn-lined GOOGLE CHROME
				.old-col
					a.browsers-link(href="#", target="_blank")
						span.sprite-browsers-firefox.browsericon
						span.btn-lined MOZILLA FIREFOX

				.old-col
					a.browsers-link(href="#", target="_blank")
						span.sprite-browsers-opera.browsericon
						span.btn-lined OPERA

